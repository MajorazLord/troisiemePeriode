<Window x:Class="VueResultat"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TracabiliteV2"
        mc:Ignorable="d"
        Title="VueResultat" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="9*"/>
        </Grid.RowDefinitions>

        <StackPanel Orientation="Horizontal">
            <Label x:Name="LbTitreNumEtiq" Content="Numéro de l'étiquette :" />
            <Label x:Name="LbNumEtiq"/>
        </StackPanel>
        <StackPanel x:Name="PResultat" Grid.Row="1"/>
        

    </Grid>
</Window>
